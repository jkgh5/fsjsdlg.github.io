            },
            {
              "id": 1079420,
              "key": "404968bb-e6f9-4051-8f78-7a7495c623a5",
              "title": "ND029 DSND C2 L0 A04 Introduction To Data Streaming",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "7IN21UGQ7Sg",
